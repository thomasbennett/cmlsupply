<?xml version="1.0"?>
<config>
    <modules>
        <RicoNeitzel_VertNav>
            <version>0.1.6</version>
			<depends>
				<Mage_Core />
				<Mage_Page />
				<Mage_Catalog />
			</depends>
        </RicoNeitzel_VertNav>
    </modules>

    <global>
    	<blocks>
    		<vertnav>
				<class>RicoNeitzel_VertNav_Block</class>
    		</vertnav>
    	</blocks>
    </global>
    <frontend>
        <layout>
            <updates>
                <vertnav>
                    <file>vertnav.xml</file>
                </vertnav>
            </updates>
        </layout>
    </frontend>
</config>

